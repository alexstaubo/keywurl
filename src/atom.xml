---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
  <title>Keywurl News</title>
  <link href="http://alexstaubo.github.com/keywurl/atom.xml" rel="self"/>
  <link href="http://alexstaubo.github.com/keywurl/"/>
  <updated>{{ site.time | date_to_xmlschema }}</updated>
  <id>http://alexstaubo.github.com/keywurl/</id>
  <author>
    <name>Alexander Staubo</name>
    <email>alex@purefiction.net</email>
  </author>
  {% for post in site.posts %}
    <entry>
      <title>{{ post.title }}</title>
      <link href="http://alexstaubo.github.com/keywurl/{{ post.url }}"/>
      <updated>{{ post.date | date_to_xmlschema }}</updated>
      <id>http://alexstaubo.github.com/keywurl/{{ post.id }}</id>
      <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
  {% endfor %} 
</feed>
